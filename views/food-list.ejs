<script>
    function goBack() {
      window.history.back();
    }
</script>
  
<%- include ('./partials/messages'); %>
    <div id="dashboard-container" class="container">
        <div class="row align-items-center">
            <div class="row">
                <div class="card">
                    <div class="card-body">
                        <div class="row dashboard-row">
                            <div class="col-11">
                                <h2 class="card-title">Food list</h2>
                                <p class="card-text"><a onclick="goBack()" class="food-back-button">‚Üê Back</a></p>
                            </div>
                            <div class="col-11">
                                <p class="card-text"><ul>
                                    <% foods.forEach(function(food){ %>
                                        <li class="food-list-item">
                                            <a href="/food/<%= food._id %>"><%= food.name %> - <%= food.quantity %> <%= food.quantity_type %></a>
                                        </li>
                                    <% }); %>
                                    </ul></p>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>


